<h1>Blogs</h1>
<div class="row">
    <% blogs.forEach(blog => { %>
    <div class="col-md-4">
        <div class="card mb-4">
            <img src="<%= blog.img %>" class="card-img-top" alt="<%= blog.title %>">
            <div class="card-body">
                <h5 class="card-title"><%= blog.title %></h5>
                <p class="card-text"><%= blog.description %></p>
            </div>
        </div>
    </div>
    <% }) %>
</div>